<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/normalize.css")" rel="stylesheet" type="text/css" /> 
    <link href="@Url.Content("~/Content/bootstrap-1.3.0.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrapmvc.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.0.6-development-only.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cycle.lite.js")" type="text/javascript"></script>
</head>
<body>
    <div class="topbar">
        <div class="fill">
            <div class="container">
                <h3><a href="#">South Sound .NET Users Group</a></h3>
                <ul class="nav">
                    <li class="active">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        @RenderBody()
        <footer>
            @Raven.Client.MvcIntegration.RavenProfiler.CurrentRequestSessions()
        </footer>
    </div>
</body>
</html>
